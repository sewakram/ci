.drop-down { 
     position: relative;  
     display: inline-block;    
     width: auto;       
     margin-top: 0;   
     font-family: verdana;    
     }      
     .drop-down select {   
     display: none;    
     }      
.drop-down .select-list {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 1;
	margin-top: 33px;
	padding: 0;
	background-color: #595959;
	overflow-y: scroll;
	height: 150px;
	border: 1px solid #d4d4d4;  
     display: none;      
     }    
.drop-down .select-list li span {
	display: block;
	min-width: 240px;
	width: 100%;
	padding: 1px 15px 1px 42px;
	background-color: #f6f4f5;
	background-position: left 8px center;
	background-repeat: no-repeat;
	font-size: 12px;
	text-align: left;
	color: #150202; 
	box-sizing: border-box;cursor: pointer;
}
.drop-down .select-list li:hover span {
	color: #fff;
	background-color: #0b559c;
}    
.menubox .menu .clsAnchor {
	line-height: 22px;
	margin: 0;
	padding: 0;
	font-size: 13px;
	display: block;
}
     .drop-down .select-list li span:hover,  
     .drop-down .select-list li span:focus {     
     opacity: 1;     
     }
     .drop-option{
      background-repeat: no-repeat;
      background-size: 25px 33px;
     }
.drop-selected {
	background-repeat: no-repeat;
	background-size: 25px 18px;
	padding: 0 0 0 35px;
	background-position: 5px;
}
.select-list-link.fa.fa-angle-down {
	font-size: 19px;
	padding: 0 5PX;
	color: #fff;
}
.drop-down .button {
	background: #0b559c;
	color: #fff;
	margin: 0 5px;
	border-radius: 3px;
	font-size: 13px;
	line-height: 28px;
}
.dropdown-lang select {
	height: 28px;
}

